{
  "name": "high-thrills-webapp",
  "version": "0.0.2",
  "description": "A Theme Park entertainment website",
  "main": "index.js",
  "scripts": {
    "setup": "npm i && npm run build:prefix",
    "start": "npm run build:prefix && concurrently \"start http://localhost:3000\" \"node .\"",
    "dev:start": "concurrently \"npm run dev:scss\" \"nodemon . dev\" \"start http://localhost:3000\"",
    "dev:scss": "onchange \"../public/SCSS/*.scss\" -- npm run build:prefix",
    "dev:db": "nodemon . emulator",
    "dev:emulators": "cd firebase && firebase emulators:start",
    "build:scss": "node-sass --recursive --output-style expanded --output ../public/CSS --source-map true --source-map-contents ../public/SCSS",
    "build:prefix": "npm run build:scss && postcss ../public/CSS/*.css --replace --use=autoprefixer"
  },
  "keywords": [],
  "author": "Joshua Pelealu, Ryan Strunk",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.1.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.3.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "onchange": "^7.1.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "socket.io": "^3.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "firebase": "^8.2.0",
    "firebase-admin": "^9.4.2",
    "glob": "^7.1.6"
  }
}
